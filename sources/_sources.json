{
  "freelaw": {
    "decisionDatesCSV": "/sources/freelaw/decisionDates.csv"
  },
  "justia": {
    "download": {
      "volume": {
        "start": 1,
        "stop": 586,
        "url": "https://supreme.justia.com/cases/federal/us/%d/",
        "dir": "/sources/justia/volumes/",
        "file": "us%03d.html"
      }
    }
  },
  "loc": {
    "download": {
      "volume100": {
        "start": 1,
        "stop": 100,
        "url": "https://www.loc.gov/collections/united-states-reports/?fa=partof:u.s.+reports:+volume+%d&st=list&c=300",
        "dir": "/sources/loc/volumes/001-100/%03d",
        "file": "us%03d.html"
      },
      "volume200": {
        "start": 101,
        "stop": 200,
        "url": "https://www.loc.gov/collections/united-states-reports/?fa=partof:u.s.+reports:+volume+%d&st=list&c=300",
        "dir": "/sources/loc/volumes/101-200/%03d",
        "file": "us%03d.html"
      },
      "volume300": {
        "start": 201,
        "stop": 300,
        "url": "https://www.loc.gov/collections/united-states-reports/?fa=partof:u.s.+reports:+volume+%d&st=list&c=300",
        "dir": "/sources/loc/volumes/201-300/%03d",
        "file": "us%03d.html"
      },
      "volume400": {
        "start": 301,
        "stop": 400,
        "url": "https://www.loc.gov/collections/united-states-reports/?fa=partof:u.s.+reports:+volume+%d&st=list&c=300",
        "dir": "/sources/loc/volumes/301-400/%03d",
        "file": "us%03d.html"
      },
      "volume500": {
        "start": 401,
        "stop": 500,
        "url": "https://www.loc.gov/collections/united-states-reports/?fa=partof:u.s.+reports:+volume+%d&st=list&c=300",
        "dir": "/sources/loc/volumes/401-500/%03d",
        "file": "us%03d.html"
      },
      "volume600": {
        "start": 501,
        "stop": 542,
        "url": "https://www.loc.gov/collections/united-states-reports/?fa=partof:u.s.+reports:+volume+%d&st=list&c=300",
        "dir": "/sources/loc/volumes/501-600/%03d",
        "file": "us%03d.html"
      },
      "pdf": {
        "info": "/sources/loc/volumes/pdfinfo.txt",
        "url": "http://cdn.loc.gov/service/ll/usrep/usrep%03d/usrep%03d%03d/usrep%03d%03d.pdf",
        "dir": "/sources/loc/volumes/%03d-%03d/%03d/",
        "file": "%03dus%03d.pdf"
      }
    }
  },
  "oyez": {
    "cases": "/sources/oyez/cases/_cases.json",
    "advocates": "/sources/oyez/advocates/_advocates.json",
    "courtsXML": "/sources/oyez/courts/**/court.xml",
    "courtsHTML": "/sources/oyez/justices_by_court.html",
    "justicesXML": "/sources/oyez/justices/**/justice.xml"
  },
  "oyezlabs": {
    "casesCSV": "/sources/sites/oyezlabs.org/cases.csv",
    "casesXML": "/sources/sites/oyezlabs.org/xml/cases/$1/**/case.xml"
  },
  "scdb": {
    "dateEnd": "2018-10-01",
    "vars": "/sources/scdb/vars.json",
    "courtsCSV": "/sources/scdb/naturalCourts.csv",
    "decisionsCSV": "/sources/scdb/decisions.csv",
    "justicesCSV": "/sources/scdb/justices.csv",
    "docketsCSV": "/sources/scdb/SCDB_2018_02_caseCentered_Docket.csv"
  },
  "scotus": {
    "citationsHTML": "/sources/scotus/citations/*.*",
    "transcripts": "/sources/scotus/transcripts/**/*.txt",
    "download": {
      "transcriptOld": {
        "start": 1968,
        "stop": 1999,
        "url": "https://www.supremecourt.gov/oral_arguments/archived_transcripts/%d",
        "dir": "/sources/scotus/transcripts/%d",
        "file": "%d.html"
      },
      "transcriptNew": {
        "start": 2000,
        "stop": 2018,
        "url": "https://www.supremecourt.gov/oral_arguments/argument_transcript/%d",
        "dir": "/sources/scotus/transcripts/%d",
        "file": "%d.html"
      },
      "transcriptPDF": {
        "files": "/sources/scotus/transcripts/$1/*.html",
        "pattern": "<td style=\"text-align: left;\">\\s*<(?:div|span)[^>]*>\\s*<a href='([^']*)'[^>]*>\\s*(.*?)\\s*</a>\\s*</(?:div|span)>\\s*<(?:div|span)[^>]*>\\s*([^<]*)\\s*</(?:div|span)>\\s*</td>\\s*<td[^>]*>\\s*([0-9]+)/([0-9]+)/([0-9]+)\\s*</td>",
        "transform": {
          "$2": ["^([0-9]+)(\\s*|-)Orig\\.?$", "$1-orig"],
          "$4": "%02d",
          "$5": "%02d",
          "$6": ["^(0|1|2)([0-9])$", "20$1$2"]
        },
        "host": "https://www.supremecourt.gov$1",
        "url": "https://www.supremecourt.gov/oral_arguments/argument_transcript/$1",
        "file": "$2_$6-$4-$5.pdf",
        "urlExclusions": [
          "https://www.supremecourt.gov/pdfs/transcripts/1998/98-405_98-406_04-26-1999.pdf"
        ],
        "urlCollisions": [
          [
            "https://www.supremecourt.gov/pdfs/transcripts/1970/70-13_10-20-1970_A.pdf",
            "https://www.supremecourt.gov/pdfs/transcripts/1970/70-13_10-20-1970_B.pdf"
          ]
        ],
        "notes": [
          "Use --group=year and/or --limit=n to limit SCOTUS downloads (and sleep briefly between each run), otherwise the supremecourt.gov server may block your IP address for a short period of time.",
          "We have also added caseId, usCite and dateDecision fields as placeholders, so that when matchTranscript() -- or the user -- matches a transcript to an SCDB entry or a SCOTUS citation, it can be properly recorded."
        ],
        "mappingCols": "term,usCite,caseTitle,caseId,docket,dateArgument,dateDecision,url,file,urlOyez,notes\n",
        "mappingData": "\"$term\",\"\",\"$3\",\"\",\"$2\",\"$6-$4-$5\",\"\",\"$url\",\"$file\",\"\",\"\"\n",
        "mappingFile": "/results/transcripts.csv"
      }
    }
  }
}

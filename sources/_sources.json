  "scotus": {
    "citationsHTML": "/sources/scotus/citations/*.*",
    "journals": "/sources/scotus/journals/*.txt",
    "transcripts": "/sources/scotus/transcripts/arguments/**/*.txt",
    "download": {
      "journal1889": {
        "start": 1889,
        "stop": 1992,
        "url": "https://www.supremecourt.gov/pdfs/journals/scannedjournals/%d_journal.pdf",
        "dir": "/sources/scotus/journals",
        "file": "%d.pdf"
      },
      "journal1993": {
        "start": 93,
        "stop": 99,
        "url": "https://www.supremecourt.gov/orders/journal/jnl%d.pdf",
        "dir": "/sources/scotus/journals",
        "file": "19%d.pdf"
      },
      "journal2000": {
        "start": 0,
        "stop": 9,
        "url": "https://www.supremecourt.gov/orders/journal/jnl0%d.pdf",
        "dir": "/sources/scotus/journals",
        "file": "200%d.pdf"
      },
      "journal2010": {
        "start": 10,
        "stop": 14,
        "url": "https://www.supremecourt.gov/orders/journal/jnl%d.pdf",
        "dir": "/sources/scotus/journals",
        "file": "20%d.pdf"
      },
      "journal2015": {
        "start": 15,
        "stop": 18,
        "url": "https://www.supremecourt.gov/orders/journal/Jnl%d.pdf",
        "dir": "/sources/scotus/journals",
        "file": "20%d.pdf"
      },
      "transcriptOld": {
        "start": 1968,
        "stop": 1999,
        "url": "https://www.supremecourt.gov/oral_arguments/archived_transcripts/%d",
        "dir": "/sources/scotus/transcripts/arguments/%d",
        "file": "%d.html"
      },
      "transcriptNew": {
        "start": 2000,
        "stop": 2018,
        "force": 2018,
        "url": "https://www.supremecourt.gov/oral_arguments/argument_transcript/%d",
        "dir": "/sources/scotus/transcripts/arguments/%d",
        "file": "%d.html"
      },
      "transcriptPDF": {
        "files": "/sources/scotus/transcripts/arguments/$1/*.html",
        "pattern": "<td style=\"text-align: left;\">\\s*<(?:div|span)[^>]*>\\s*<a href='([^']*)'[^>]*>\\s*(.*?)\\s*</a>\\s*</(?:div|span)>\\s*<(?:div|span)[^>]*>\\s*([^<]*)\\s*</(?:div|span)>\\s*</td>\\s*<td[^>]*>\\s*([0-9]+)/([0-9]+)/([0-9]+)\\s*</td>",
        "transform": {
          "$2": ["^([0-9]+)(\\s*|-)Orig\\.?$", "$1-orig"],
          "$4": "%02d",
          "$5": "%02d",
          "$6": ["^(0|1|2)([0-9])$", "20$1$2"]
        },
        "host": "https://www.supremecourt.gov$1",
        "url": "https://www.supremecourt.gov/oral_arguments/argument_transcript/$1",
        "file": "$2_$6-$4-$5.pdf",
        "urlExclusions": [
          "https://www.supremecourt.gov/pdfs/transcripts/1998/98-405_98-406_04-26-1999.pdf"
        ],
        "urlCollisions": [
          [
            "https://www.supremecourt.gov/pdfs/transcripts/1970/70-13_10-20-1970_A.pdf",
            "https://www.supremecourt.gov/pdfs/transcripts/1970/70-13_10-20-1970_B.pdf"
          ]
        ],
        "notes": [
          "Use --group=year and/or --limit=n to limit SCOTUS downloads (and sleep briefly between each run), otherwise the supremecourt.gov server may block your IP address for a short period of time.",
          "We have also added caseId, usCite and dateDecision fields as placeholders, so that when matchTranscript() -- or the user -- matches a transcript to an SCDB entry or a SCOTUS citation, it can be properly recorded."
        ],
        "mappingCols": "term,usCite,caseTitle,caseId,docket,dateArgument,dateDecision,url,file,urlOyez,notes\n",
        "mappingData": "\"$term\",\"\",\"$3\",\"\",\"$2\",\"$6-$4-$5\",\"\",\"$url\",\"$file\",\"\",\"\"\n",
        "mappingFile": "/sources/ld/transcripts.csv"
      }
    }
  }
}

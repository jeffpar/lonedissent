{
  "ID": 55358,
  "name": "Lance v. Dennis",
  "href": "https://api.oyez.org/cases/2005/05-555",
  "view_count": 0,
  "docket_number": "05-555",
  "additional_docket_numbers": null,
  "manner_of_jurisdiction": "Appeal",
  "first_party": "Keith Lance, et al.",
  "second_party": "Gigi Dennis, Colorado Secretary of State",
  "timeline": [
    {

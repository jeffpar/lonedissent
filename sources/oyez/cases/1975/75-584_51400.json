{
  "ID": 51400,
  "name": "Federal Power Commission v. Transcontinental Gas Pipe Line Corporation",
  "href": "https://api.oyez.org/cases/1975/75-584",
  "view_count": 0,
  "docket_number": "75-584",
  "additional_docket_numbers": null,
  "manner_of_jurisdiction": "Writ of <i>certiorari</i>",
  "first_party": "Federal Power Commission",
